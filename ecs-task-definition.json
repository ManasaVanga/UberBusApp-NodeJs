{
    "family": "uber_bus_app_be", 
    "placementConstraints": [],  
    "executionRoleArn": "arn:aws:iam::005299268433:role/ecsTaskExecutionRole", 
         
        
    "taskRoleArn": "arn:aws:iam::005299268433:role/ecsTaskExecutionRole",  
    "containerDefinitions": [
        {
             
            "name": "uber_bus_app_be", 
            "image": "005299268433.dkr.ecr.us-east-1.amazonaws.com/uber_bus_app_be:latest", 
            "portMappings": [
                {
                    "protocol": "tcp", 
                    "containerPort": 80, 
                    "hostPort": 80
                }
            ], 
            "logConfiguration": {
                "logDriver": "awslogs", 
                    "options": {
                        "awslogs-region": "us-east-1", 
                        "awslogs-stream-prefix": "ecs", 
                        "awslogs-group": "/ecs/uber_bus_app_be"
                    }
            }, 
            "essential": true 
                
        }
    ],
    "cpu":"512",
    "memory":"1024"
    
    
}
